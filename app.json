{
  "expo": {
    "name": "travel-ai",
    "slug": "travel-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "travel-ai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ludwigw.travelai",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "location"
        ]
      },
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ludwigw.travelai",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "We need your location to provide you with relevant information about your surroundings."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactServerComponentRoutes": true,
      "reactServerFunctions": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8a8e7051-a2cc-491c-89ec-cf6d50a47094"
      }
    }
  }
}